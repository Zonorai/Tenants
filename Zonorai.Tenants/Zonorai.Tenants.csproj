<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <PackageId>Zonorai.Tenants</PackageId>
    <Version>1.0.2</Version>
    <Authors>Carel vd Merwe</Authors>
    <Company>Zonorai</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>A Package consuming Finbuckle.MultiTenant using Claim Strategy and EF Store</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Finbuckle.MultiTenant" Version="6.5.0" />
    <PackageReference Include="Finbuckle.MultiTenant.AspNetCore" Version="6.5.0" />
    <PackageReference Include="Finbuckle.MultiTenant.EntityFrameworkCore" Version="6.5.0" />
    <PackageReference Include="FluentValidation" Version="10.3.4" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="5.0.12">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="5.0.12" />
  </ItemGroup>

</Project>
